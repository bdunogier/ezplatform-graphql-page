{
    "name": "bdunogier/ezplatform-graphql-page",
    "description": "GraphQL integration of the eZ Platform Page feature",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Bertrand Dunogier",
            "email": "bertrand.dunogier@ez.no"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ezsystems/ezplatform-page-fieldtype": "^1.0",
        "bdunogier/ezplatform-graphql-bundle": "^0.4@dev"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "BD\\EzPlatformGraphQLPageBundle\\": "src/bundle",
            "BD\\EzPlatformGraphQLPage\\": "src/lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\BD\\EzPlatformGraphQLPageBundle\\": "spec/bundle",
            "spec\\BD\\EzPlatformGraphQLPage\\": "spec/lib"
        }
    },
    "repositories": [
        { "type": "composer", "url": "https://updates.ez.no/ttl" }
    ]
}
