{
    "name": "bdunogier/ezplatform-graphql-page",
    "description": "GraphQL integration of the eZ Platform Page feature",
    "type": "ezplatform-bundle",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Bertrand Dunogier",
            "email": "bertrand.dunogier@ez.no"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "ezsystems/ezplatform-page-fieldtype": "^1.0",
        "ezsystems/ezplatform-graphql": "^0.4.0@beta"
    },
    "require-dev": {
        "phpspec/phpspec": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "BD\\EzPlatformGraphQLPage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\BD\\EzPlatformGraphQLPage\\": "spec",
            "spec\\EzSystems\\EzPlatformGraphQL\\Tools\\": "vendor/ezsystems/ezplatform-graphql/spec/Tools"
        }
    },
    "repositories": [
        { "type": "composer", "url": "https://updates.ez.no/ttl" }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    }
}
